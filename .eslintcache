nent/GreetingComponent2\";\nimport ProfileComponent from \"./componentes/apicontext/ProfileComponent\";\nimport ProfileContextApi from \"./componentes/apicontext/ProfileContextApi\";\nimport ProfileUseContext from \"./componentes/apicontext/ProfileUseContext\";\nimport ProfileComponentSetState from \"./componentes/apicontext/ProfileComponentSetState\";\nimport ProfileComponentUseContext from \"./componentes/apicontext/ProfileComponentUseContext\";\nimport ProfileContextHook from \"./componentes/apicontext/ProfileContextHook\";\nimport StorageHook from \"./componentes/hooks/StorageHook\";\nimport CacheStorageHook from \"./componentes/hooks/CacheStorageHook\";\n\nfunction App() {\n  return (\n    <div>\n      <nav className=\"navbar navbar-expand navbar-dark bg-dark\">\n        <a href=\"/tutorials\" className=\"navbar-brand\">\n          React.js\n        </a>\n        <div className=\"navbar-nav mr-auto\">\n        <li class=\"nav-item dropdown\">\n          <a class=\"nav-link dropdown-toggle\" href=\"#\" id=\"navbarDropdown\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\n            Class Component\n          </a>\n          <ul class=\"dropdown-menu bg-dark\" aria-labelledby=\"navbarDropdown\">\n            <li><Link to={\"/counterComponente\"} className=\"nav-link\">\n              Contador (state)\n            </Link></li>\n            <li><Link to={\"/counterComponente2\"} className=\"nav-link\">\n              Contador (ComponentDidMount)\n            </Link></li>\n            <li><Link to={\"/greetingComponent\"} className=\"nav-link\">\n              Saudação (ComponentDidMount)\n            </Link></li>\n            <li><Link to={\"/cacheStorageHook\"} className=\"nav-link\">\n              Saudação (LocalStorage)\n            </Link></li>\n          </ul>\n        </li>\n        <li class=\"nav-item dropdown\">\n          <a class=\"nav-link dropdown-toggle\" href=\"#\" id=\"navbarDropdown\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\n            Hook\n          </a>\n          <ul class=\"dropdown-menu bg-dark\" aria-labelledby=\"navbarDropdown\">\n            <li><Link to={\"/counterHook\"} className=\"nav-link\">\n              Counter (useState)\n            </Link></li>\n            <li><Link to={\"/counterHook2\"} className=\"nav-link\">\n              Contador (useEffect)\n            </Link></li>\n            <li><Link to={\"/greetingHook\"} className=\"nav-link\">\n              Saudação (useEffect)\n            </Link></li>\n            <li><Link to={\"/greetingHook2\"} className=\"nav-link\">\n              Saudação (LocalStorage)\n            </Link></li>\n            <li><Link to={\"/storageHook\"} className=\"nav-link\">\n              Storage (Custom Hook)\n            </Link></li>\n            <li><Link to={\"/cacheStorageHook\"} className=\"nav-link\">\n              Cache Storage (LocalStorage)\n            </Link></li>\n          </ul>\n        </li>\n        <li class=\"nav-item dropdown\">\n          <a class=\"nav-link dropdown-toggle\" href=\"#\" id=\"navbarDropdown\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\n            Context API\n          </a>\n          <ul class=\"dropdown-menu bg-dark\" aria-labelledby=\"navbarDropdown\">\n            <li><Link to={\"/profileComponent\"} className=\"nav-link\">\n              Perfil (Component)\n            </Link></li>\n            <li><Link to={\"/profileComponent\"} className=\"nav-link\">\n              Perfil (Context API)\n            </Link></li>\n            <li><Link to={\"/profileComponent\"} className=\"nav-link\">\n              Perfil (useContext)\n            </Link></li>\n            <li><Link to={\"/profileComponentSetState\"} className=\"nav-link\">\n              Perfil (Component setState)\n            </Link></li>\n            <li><Link to={\"/profileComponentUseContext\"} className=\"nav-link\">\n              Perfil (useContext setState)\n            </Link></li>\n            <li><Link to={\"/profileContextHook\"} className=\"nav-link\">\n              Perfil (Hook useContext)\n            </Link></li>\n          </ul>\n        </li>\n        </div>\n      </nav>\n\n      <div className=\"container\">\n        <Switch>\n          <Route exact path=\"/counterComponente\" component={CounterComponent} />\n          <Route exact path=\"/counterHook\" component={CounterHook} />\n          <Route exact path=\"/counterComponente2\" component={CounterComponent2} />\n          <Route exact path=\"/counterHook2\" component={CounterHook2} />\n          <Route exact path=\"/greetingComponent\" component={GreetingComponent} />\n          <Route exact path=\"/greetingHook\" component={GreetingHook} />\n          <Route exact path=\"/greetingComponent2\" component={GreetingComponent2} />\n          <Route exact path=\"/greetingHook2\" component={GreetingHook2} />\n          <Route exact path=\"/profileComponent\" component={ProfileComponent} />\n          <Route exact path=\"/profileContextApi\" component={ProfileContextApi} />\n          <Route exact path=\"/profileUseContext\" component={ProfileUseContext} />\n          <Route exact path=\"/profileComponentUseContext\" component={ProfileComponentUseContext} />\n          <Route exact path=\"/profileComponentSetState\" component={ProfileComponentSetState} />\n          <Route exact path=\"/profileContextHook\" component={ProfileContextHook} />\n          <Route exact path=\"/storageHook\" component={StorageHook} />\n          <Route exact path=\"/cacheStorageHook\" component={CacheStorageHook} />\n          \n        </Switch>\n      </div>\n    </div>\n\n\n  );\n}\n\nexport default App;\n","/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/hooks/CounterHook.js",[],"/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/component/CounterComponent.js",[],"/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/hooks/CounterHook2.js",["110","111"],"import React, { Component, useState, useEffect } from 'react';\n\nfunction CounterHook2() {\n  const [count, setCount] = useState(0);\n  const incrementCount = () => setCount(count + 1);\n\n  useEffect(() => {\n    document.title = `Você clicou ${count} vezes.`\n  }, []);\n\n  return (\n    <div>\n      <p>Você clicou {count} vezes</p>\n      <button onClick={incrementCount}>Clique aqui</button>\n    </div>\n  )\n}\n\nexport default CounterHook2\n","/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/component/CounterComponent2.js",[],"/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/hooks/GreetingHook.js",["112","113"],"import React, { Component, useState, useEffect } from 'react';\nexport default function Greeting() {\n  \n  const [firstName, setFirstName] = useState(\"Bat\");\n  const [lastName, setLastName] = useState(\"Man\");\n  const [msg, setMsg] = useState();\n\n  const handleFirstNameChange = (e) => setFirstName(e.target.value);\n  const handleLastNameChange = (e) => setLastName(e.target.value);\n\n  useEffect(() => {\n    setMsg(firstName + \" \" + lastName)\n  }, [lastName]) \n\n  return (\n    <div>\n      <input value={firstName} onChange={handleFirstNameChange} /><br />\n      <input value={lastName} onChange={handleLastNameChange} />\n      <p>\n        Oi, <span>{firstName} {lastName}</span> <br/>\n      </p>\n      <p>\n        Msg: <span>{msg}</span> <br/>\n        \n      </p>\n    </div>\n  );\n}","/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/component/GreetingComponent.js",["114"],"import React, { Component } from 'react';\n\nexport default class GreetingComponent extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      firstName: 'Harry',\n      lastName: 'Poppins'\n    };\n    this.handleFirstNameChange = this.handleFirstNameChange.bind(this);\n    this.handleLastNameChange = this.handleLastNameChange.bind(this);\n  }\n\n  handleFirstNameChange = (e) => this.setState({ firstName: e.target.value });\n  handleLastNameChange = (e) => this.setState({ lastName: e.target.value });\n\n  render() {\n    return (\n      <div>\n        <input value={this.state.firstName} onChange={this.handleFirstNameChange}/><br />\n        <input value={this.state.lastName} onChange={this.handleLastNameChange}/>\n        <p>\n          <span>Oi, {this.state.firstName} {this.state.lastName}</span>\n        </p>\n      </div>\n    );\n  }\n}","/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/hooks/GreetingHook2.js",["115"],"import React, { Component, useState, useEffect } from 'react';\nexport default function GreetingHook2() {\n\n  const [firstName, setFirstName] = useState(() =>\n    localStorage.getItem('hooksFirstName') || ''\n  );\n  const [lastName, setLastName] = useState(() =>\n    localStorage.getItem('hooksLastName') || ''\n  );\n\n  const handleFirstNameChange = (e) => setFirstName(e.target.value);\n  const handleLastNameChange = (e) => setLastName(e.target.value);\n  const resetState = (e) => {\n    setFirstName('')\n    setLastName('')\n  }\n\n  useEffect(() => {\n    localStorage.setItem('hooksFirstName', firstName);\n    localStorage.setItem('hooksLastName', lastName);\n  });\n\n  return (\n    <div>\n      <input value={firstName} onChange={handleFirstNameChange} /><br />\n      <input value={lastName} onChange={handleLastNameChange} />\n      <p>\n        Oi, <span>{firstName} {lastName}</span>\n      </p>\n      <button onClick={resetState}>Limpar</button>\n    </div>\n  );\n}\n","/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/component/GreetingComponent2.js",["116"],"import React, { Component } from 'react';\n\nexport default class GreetingComponent2 extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      firstName: window.localStorage.getItem('classFirstName') || '',\n      lastName: window.localStorage.getItem('classLastName') || ''\n    };\n    this.handleFirstNameChange = this.handleFirstNameChange.bind(this);\n    this.handleLastNameChange = this.handleLastNameChange.bind(this);\n  }\n  handleFirstNameChange = (e) => this.setState({ firstName: e.target.value });\n  handleLastNameChange = (e) => this.setState({ lastName: e.target.value });\n\n  componentDidUpdate() {\n    window.localStorage.setItem('classFirstName', this.state.firstName);\n    window.localStorage.setItem('classLastName', this.state.lastName) ;\n  }\n\n  render() {\n    return (\n      <div>\n        <input value={this.state.firstName} onChange={this.handleFirstNameChange} /><br />\n        <input value={this.state.lastName} onChange={this.handleLastNameChange} />\n        <p>\n          <span>Oi, {this.state.firstName} {this.state.lastName}</span>\n        </p>\n      </div>\n    );\n  }\n}\n","/home/totemarcal/ReactProjects/CursoSolutis/curso/src/componentes/apicontext/ProfileContextApi/index.js",["117","118"],"import React from 'react';\nimport './style.css';\nimport ProfileProvider from './ProfileProvider'\n\nconst ProfileContext = React.createContext();\n\nconst Profile = () => (\n  <div className=\"profile\">\n    <ProfileContext.Consumer>\n      {context => <img src={context.companyImage} />}\n    </ProfileContext.Consumer>\n    <User />\n  </div>\n)\n\nconst User = () => (\n  <div className=\"user\">\n    <ProfileContext.Consumer>\n      {context =>\n        <React.Fragment>\n          <a href={context.url}>\n            <img src={context.userImage} width=\"138px\" />\n          </a>\n          <h1 className=\"profile-userName\">{context.userName}</h1>\n          <p className=\"profile-fullName\">({context.fullName})</p>\n        </React.Fragment>\n      }\n    </ProfileContext.Consumer>\n    <Team />\n  </div>\n)\n\nconst Team = () => (\n  <ProfileContext.Consumer>\n    {context =>\n      <div className=\"team\">\n        <p className=\"profile-team\">{context.team}</p>\n      </div>\n    }\n  </ProfileContext.Consumer>\n)\